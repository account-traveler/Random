function RUN()
print("RUN")
end

function ESCAPE()
print("ESCAPE")
end

function STOP()
print("STOP")
end

function ANTISKIDDING()
print("ANTI-SKIDDING")
end

task.spawn(function()
         while task.wait() do
                  RUN()
                  ESCAPE()
                  STOP()
                  ANTISKIDDING()
         end
end)

